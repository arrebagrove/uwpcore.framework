<?xml version="1.0" encoding="utf-8"?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.1">
  <CommandSet xml:lang="en-us" Name="AdventureWorksCommandSet_en-us">
    <CommandPrefix> Adventure Works, </CommandPrefix>
    <Example> Show trip to London </Example>

    <Command Name="showTripToDestination">
      <Example> show trip to London  </Example>
      <ListenFor> show trip to {destination} </ListenFor>
      <Feedback> Showing trip to {destination} </Feedback>
      <Navigate/>
    </Command>

    <PhraseList Label="destination">
      <Item> London </Item>
      <Item> Dallas </Item>
      <Item> New York </Item>
      <Item> Berlin </Item>
      <Item> Munich </Item>
    </PhraseList>

  </CommandSet>
  
  <CommandSet xml:lang="de-de" Name="AdventureWorksCommandSet_de-de">
    <CommandPrefix> Abenteuer Fabrik, </CommandPrefix>
    <Example> Zeige Trips nach London </Example>

    <Command Name="showTripToDestination">
      <Example> Zeige Trips nach London  </Example>
      <ListenFor> Zeige Trips nach {destination} </ListenFor>
      <Feedback> Ich zeige dir die Trips nach {destination} </Feedback>
      <Navigate/>
    </Command>

    <PhraseList Label="destination">
      <Item> London </Item>
      <Item> Dallas </Item>
      <Item> New York </Item>
      <Item> Berlin </Item>
      <Item> München </Item>
    </PhraseList>

  </CommandSet>

  <!-- Other CommandSets for other languages -->

</VoiceCommands>